<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.EntRealizadoNAV.EntRealizadoFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerview_ent_realizado"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_vacio"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/no_has_realizado_ning_n_entrenamiento"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:textSize="17sp"
        android:visibility="gone"
        app:layout_constraintTop_toTopOf="@+id/recyclerview_ent_realizado" />
</androidx.constraintlayout.widget.ConstraintLayout>